@model List<ProductViewModel>


  <section style="background-color: #eee;">
        <div class="text-center container py-5">
            <h4 class="mt-4 mb-5"><strong>Popüler Ürünler</strong></h4>
            <div class="row">
                @foreach(var item in Model){
                @await Html.PartialAsync("_popular-products",item)
                }
            </div>
        </div>
    </section>
    

